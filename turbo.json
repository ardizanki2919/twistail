{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [".next/**", "dist/**", "out/**"]
        },
        "lint": {},
        "check": {},
        "typecheck": {},
        "cleanup": {
            "cache": false
        },
        "start": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "storybook": {
            "cache": false,
            "persistent": true
        }
    }
}
