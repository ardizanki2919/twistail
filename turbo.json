{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [".next/**", "dist/**", "out/**"]
        },
        "lint": {},
        "check": {},
        "typecheck": {
            "dependsOn": ["twistail-utils#build"]
        },
        "cleanup": {
            "cache": false
        },
        "start": {
            "cache": false
        },
        "dev": {
            "dependsOn": ["twistail-utils#build"],
            "persistent": true,
            "cache": false
        },
        "storybook": {
            "persistent": true,
            "cache": false
        },
        "publish:dry": {}
    }
}
