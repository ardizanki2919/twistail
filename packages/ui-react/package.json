{
    "name": "twistail-react",
    "version": "0.0.6",
    "type": "module",
    "description": "Twistail UI Components",
    "author": "Aris Ripandi <aris@duck.com>",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "https://github.com/riipandi/twistail/blob/main/LICENSE-APACHE"
        },
        {
            "type": "MIT",
            "url": "https://github.com/riipandi/twistail/blob/main/LICENSE-MIT"
        }
    ],
    "homepage": "https://twistail.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/riipandi/twistail"
    },
    "bugs": {
        "url": "https://github.com/riipandi/twistail/issues"
    },
    "scripts": {
        "build": "vite build",
        "start": "pnpm dlx local-web-server -p 6006 -d storybook-static",
        "storybook": "VITE_CJS_IGNORE_WARNING=true storybook dev -p 6006 --no-open",
        "build:storybook": "VITE_CJS_IGNORE_WARNING=true storybook build -o storybook-static",
        "cleanup": "pnpm dlx rimraf dist node_modules storybook-static",
        "publish:dry": "pnpm publish --dry-run --no-git-checks",
        "prepublishOnly": "pnpm -s build",
        "lint": "biome lint . --write",
        "check": "biome check . --write",
        "typecheck": "tsc --noEmit"
    },
    "exports": {
        ".": "./dist/index.js",
        "./accordion": "./dist/base-ui/accordion/index.js",
        "./avatar": "./dist/base-ui/avatar/index.js",
        "./badge": "./dist/base-ui/badge/index.js",
        "./breadcrumb": "./dist/base-ui/breadcrumb/index.js",
        "./button": "./dist/base-ui/button/index.js",
        "./calendar": "./dist/base-ui/calendar/index.js",
        "./callout": "./dist/base-ui/callout/index.js",
        "./card": "./dist/base-ui/card/index.js",
        "./checkbox": "./dist/base-ui/checkbox/index.js",
        "./context-menu": "./dist/base-ui/context-menu/index.js",
        "./date-picker": "./dist/base-ui/date-picker/index.js",
        "./dialog": "./dist/base-ui/dialog/index.js",
        "./divider": "./dist/base-ui/divider/index.js",
        "./drawer": "./dist/base-ui/drawer/index.js",
        "./dropdown-menu": "./dist/base-ui/dropdown-menu/index.js",
        "./heading": "./dist/base-ui/heading/index.js",
        "./hover-card": "./dist/base-ui/hover-card/index.js",
        "./input": "./dist/base-ui/input/index.js",
        "./label": "./dist/base-ui/label/index.js",
        "./listbox": "./dist/base-ui/listbox/index.js",
        "./pagination": "./dist/base-ui/pagination/index.js",
        "./popover": "./dist/base-ui/popover/index.js",
        "./radio-card-group": "./dist/base-ui/radio-card-group/index.js",
        "./radio-group": "./dist/base-ui/radio-group/index.js",
        "./scroll-area": "./dist/base-ui/scroll-area/index.js",
        "./select": "./dist/base-ui/select/index.js",
        "./skeleton": "./dist/base-ui/skeleton/index.js",
        "./slider": "./dist/base-ui/slider/index.js",
        "./switch": "./dist/base-ui/switch/index.js",
        "./tab-navigation": "./dist/base-ui/tab-navigation/index.js",
        "./table": "./dist/base-ui/table/index.js",
        "./tabs": "./dist/base-ui/tabs/index.js",
        "./text": "./dist/base-ui/text/index.js",
        "./textarea": "./dist/base-ui/textarea/index.js",
        "./toast": "./dist/base-ui/toast/index.js",
        "./toggle": "./dist/base-ui/toggle/index.js",
        "./tooltip": "./dist/base-ui/tooltip/index.js",
        "./global.css": "./dist/styles/global.css",
        "./theme.css": "./dist/styles/theme.css"
    },
    "files": [
        "dist/**/*",
        "!tests/**/*",
        "!dist/**/*.*.map",
        "!tests/**/*.*.map",
        "package.json",
        "LICENSE-APACHE",
        "LICENSE-MIT",
        "README.md"
    ],
    "peerDependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "dependencies": {
        "@internationalized/date": "^3.7.0",
        "@react-aria/datepicker": "^3.14.1",
        "@react-stately/datepicker": "^3.13.0",
        "date-fns": "4.1.0",
